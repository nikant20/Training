<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
	<h2>Learning JSTL</h2>
	<h3>Through EL</h3>
	<p>Name: ${key1.username} and Age: ${key1.age}</p>
	<p>Name: ${key2.username} and Age: ${key2.age}</p>
	<h3>Through JSTL</h3>
	<%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="core" %>
	 <core:set var="user1" value="${key1}"></core:set>
	 <core:set var="user1" value="${key2}"></core:set>
</body>
</html>